	"memo arePropsEqual": {
		"prefix": "memo-arePropsEqual",
		"body": [
			"import { memo } from 'react';",
			"",
			"const ${1:${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/g}} = memo(function $1($2) {",
			"\t$0",
			"",
			"\treturn (",
			"\t\t$4",
			"\t);",
			"}, arePropsEqual);",
			"",
